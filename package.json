        "inversify": "^5.0.1",
        "node-fetch": "^2.6.0",
        "reflect-metadata": "0.1.13",
        "vee-validate": "^3.2.5",
        "vue": "2.6.11",
        "vue-class-component": "^7.1.0",
        "vue-property-decorator": "8.4.0",
        "vue-router": "3.1.6",
        "vuex": "^3.1.2",
        "whatwg-fetch": "~3.0.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "2.21.0",
